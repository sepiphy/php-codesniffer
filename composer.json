{
    "name": "xuanquynh/php-codesniffer",
    "description": "CodeSniffer for XuanQuynh Coding Recommendations.",
    "keywords": ["xuanquynh", "codesniffer", "php-codesniffer", "coding", "coding-convention"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/xuanquynh/php-codesniffer",
    "support": {
        "issues": "https://github.com/xuanquynh/php-codesniffer/issues",
        "source": "https://github.com/xuanquynh/php-codesniffer"
    },
    "authors": [
        {
            "name": "Quynh Xuan Nguyen",
            "email": "seriquynh@gmail.com",
            "homepage": "http://seriquynh.com",
            "role": "Software Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "wataridori/framgia-php-codesniffer": "^0.3"
    },
    "require-dev": {
        "composer/composer": "^1.8",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "XuanQuynh\\CodeSniffer\\": "src/"
        }
    },
    "scripts": {
        "fix": "@php vendor/bin/php-cs-fixer fix",
        "test": "@php vendor/bin/phpunit",
        "post-autoload-dump": "XuanQuynh\\CodeSniffer\\ComposerScripts::loadStandards"
    },
    "scripts-descriptions": {
        "fix": "Fix coding convention.",
        "test": "Run unit tests."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
