{
    "name": "xuanquynh/php-codesniffer",
    "description": "Personal PHP Standards Checker by PHP CodeSniffer.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Xuan Quynh",
            "email": "nguyenxuanquynh2210vghy@gmail.com"
        }
    ],
    "require": {
        "squizlabs/php_codesniffer": "^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "XuanQuynh\\CodeSniffer\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
